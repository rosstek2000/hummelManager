@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>



<div class="container">
    <div class="row">
        <div id="imgcarousel" class="carousel" data-interval="200" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-target="#imgcarousel" data-slide-to="0" class="active"></li>
                <li data-target="#imgcarousel" data-slide-to="1"></li>
                <li data-target="#imgcarousel" data-slide-to="2"></li>
                <li data-target="#imgcarousel" data-slide-to="3"></li>
            </ol>
        </div>
    </div>
</div>

<div class="carousel-inner">
    <div class="item active">
        <img src="~/Content/images/test1.png" alt="My First Image" class="img-responsive" { width:390px !important; height:360px !important;} />

    </div>

    <div class="item">
        <img src="~/Content/images/test2.png" alt="My Second Image" class="img-responsive" />
    </div>

    <div class="item">
        <img src="~/Content/images/test3.png" alt="My Third Image" class="img-responsive" />

    </div>

    <div class="item">
        <img src="~/Content/images/test4.png" alt="My Fourth Image" class="img-responsive" />
    </div>
</div>
